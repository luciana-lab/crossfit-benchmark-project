<div class="container">
    <h2>Edit <%= @user.first_name %> <%= @user.last_name %> Profile</h2>
    <hr>
    
        <%= form_for @user do |f| %>
            <%= render partial: 'form', locals: { f: f } %>
        <%= f.submit "Update Account", {class: "user-account-btn"} %>
        <% end %>
</div>