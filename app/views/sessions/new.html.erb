<h1>Log In</h1>

<%= form_for :user, url: '/login' do |f| %>

    <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email %>
    </div>

    <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password %>
    </div>

    <div class="actions">
    <%= f.submit "Log In" %>
    </div>
<% end %>